{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProductResponse",
  "type": "object",
  "required": [
    "created_at",
    "gtin",
    "measures",
    "productname",
    "sellsinraw",
    "updated_at"
  ],
  "properties": {
    "created_at": {
      "type": "string",
      "format": "partial-date-time"
    },
    "gtin": {
      "type": "string",
      "pattern": "^[0-9]{8,14}$"
    },
    "measures": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ProductToMeasureResponse"
      }
    },
    "productname": {
      "type": "string"
    },
    "sellsinraw": {
      "type": "boolean"
    },
    "sku": {
      "type": [
        "string",
        "null"
      ]
    },
    "updated_at": {
      "type": "string",
      "format": "partial-date-time"
    }
  },
  "definitions": {
    "ProductToMeasureResponse": {
      "type": "object",
      "required": [
        "amount",
        "created_at",
        "gtin",
        "id",
        "is_primary_measure",
        "unit",
        "unit_id"
      ],
      "properties": {
        "amount": {
          "type": "string",
          "pattern": "^[0-9]+(\\.[0-9]+)?$"
        },
        "created_at": {
          "type": "string",
          "format": "partial-date-time"
        },
        "gtin": {
          "type": "string",
          "pattern": "^[0-9]{8,14}$"
        },
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "is_converted": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "is_primary_measure": {
          "type": "boolean"
        },
        "raw_amount": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[0-9]+(\\.[0-9]+)?$"
        },
        "unit": {
          "$ref": "#/definitions/Unit"
        },
        "unit_id": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "Unit": {
      "type": "object",
      "required": [
        "id",
        "symbol"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int32"
        },
        "symbol": {
          "$ref": "#/definitions/UnitSymbol"
        }
      }
    },
    "UnitSymbol": {
      "type": "string",
      "enum": [
        "fl oz",
        "oz",
        "mL",
        "g"
      ]
    }
  }
}